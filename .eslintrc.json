{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:node/recommended-module",
        "plugin:unicorn/recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": [ "error", "windows" ],
        "quotes": [ "error", "double" ],
        "semi": [ "error", "always" ]
    }
}
